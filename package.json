{
  "name": "dior-host-bot",
  "version": "1.0.0",
  "description": "Telegram Bot for Dior Host",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon",
    "fix-prices": "node -e \"const fs=require('fs'),p=require('path');const d=p.join(process.cwd(),'dist'),pj=p.join(d,'helpers','prices.js'),pc=p.join(d,'helpers','prices.cjs');if(!fs.existsSync(pj)){process.exit(0);}let c=fs.readFileSync(pj,'utf-8');c=c.replace(/Object\\.defineProperty\\(exports,\\s*[\\\"']__esModule[\\\"'],\\s*\\{\\s*value:\\s*true\\s*\\}\\);?\\s*\\n?/g,'').replace(/exports\\.default\\s*=/g,'module.exports =');fs.writeFileSync(pc,c,'utf-8');fs.unlinkSync(pj);console.log('fix-prices-cjs: wrote dist/helpers/prices.cjs');function w(dir){try{fs.readdirSync(dir,{withFileTypes:true}).forEach(e=>{const f=p.join(dir,e.name);if(e.isDirectory())w(f);else if(e.name.endsWith('.js')){let ct=fs.readFileSync(f,'utf-8'),b=ct;ct=ct.replace(/require\\s*\\(\\s*([\\\"'])([^\\\"']*helpers\\/prices)(\\.js)?\\1\\s*\\)/g,(_,q,p)=>'require('+q+p+'.cjs'+q+')').replace(/require\\s*\\(\\s*([\\\"'])\\.\\/prices(\\.js)?\\1\\s*\\)/g,(_,q)=>'require('+q+'./prices.cjs'+q+')');if(ct!==b){fs.writeFileSync(f,ct,'utf-8');console.log('fix-prices-cjs: patched',p.relative(d,f));}}}catch(e){}}w(d);\"",
    "build": "tsc || echo. && tsc-alias -p tsconfig.json && npm run fix-prices",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "start": "node --no-deprecation dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "node --import tsx --test src/modules/automations/__tests__/evaluator.test.ts src/modules/automations/__tests__/nps-callback.test.ts",
    "check-payments": "ts-node scripts/check-payments.ts",
    "test-amper-api": "tsx scripts/test-amper-api.ts",
    "test-amper-register": "tsx scripts/test-amper-register.ts",
    "amper-ping": "tsx scripts/amper-ping.ts"
  },
  "keywords": [],
  "author": "flexice <two.fl3xice@yandex.ru>",
  "license": "PRIVATE",
  "repository": {
    "type": "git",
    "url": "https://github.com/pavelfrolovvvvv1234-debug/dior-bot.git"
  },
  "devDependencies": {
    "@swc/core": "^1.10.7",
    "@swc/helpers": "^0.5.15",
    "@types/express": "^5.0.0",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.10.7",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.0.1",
    "@grammyjs/fluent": "^1.0.3",
    "@grammyjs/menu": "^1.3.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/stateless-question": "3",
    "@grammyjs/storage-file": "^2.4.2",
    "@moebius/fluent": "^1.1.0",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.8.1",
    "bullmq": "^5.34.5",
    "cron-parser": "^5.5.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "grammy": "^1.34.0",
    "ioredis": "^5.4.1",
    "ms": "^2.1.3",
    "tldts": "^6.1.77",
    "typeorm": "^0.3.20",
    "whois": "^2.16.1",
    "zod": "^3.24.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "better-sqlite3",
      "sqlite3"
    ]
  }
}
